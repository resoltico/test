  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^22.14.0",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^9.24.0",
    "tsx": "^4.19.3",
